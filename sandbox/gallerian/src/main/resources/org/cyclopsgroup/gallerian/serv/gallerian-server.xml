<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="
http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-2.5.xsd">
    <bean name="gallerian.ContentListingService"
        class="org.cyclopsgroup.gallerian.serv.ServerSideContentListingService" />
    <bean class="org.springframework.jmx.export.MBeanExporter"
        lazy-init="false">
        <property name="beans">
            <map>
                <entry
                    key="org.cyclopsgroup.gallerian:type=ContentListingService"
                    value-ref="gallerian.ContentListingService" />
            </map>
        </property>
    </bean>
    <bean name="waterview.SimpleContextProcessor" class="org.cyclopsgroup.waterview.impl.pipeline.Pipeline">
        <constructor-arg>
            <list>
                <ref bean="waterview.SetGlobalVariablesValve" />
                <ref bean="waterview.ParseRequestValve" />
                <ref bean="waterview.RenderPageValve" />
            </list>
        </constructor-arg>
    </bean>
</beans>