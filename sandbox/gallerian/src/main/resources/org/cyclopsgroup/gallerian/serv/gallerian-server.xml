<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="
http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-2.5.xsd">
    <bean name="gallerian.ContentListingService"
        class="org.cyclopsgroup.gallerian.serv.ServerSideContentListingService">
        <property name="initialRepositories">
            <map>
                <entry key="home" value="vfs:/home/jiaqi" />
                <entry key="opt" value="vfs:/opt" />
            </map>
        </property>
    </bean>
    <bean name="gellerian.ContentStreamService"
        class="org.cyclopsgroup.gallerian.serv.LocalContentStreamService">
        <constructor-arg ref="gallerian.ContentListingService" />
        <property name="iconDirectory"
            value="/home/jiaqi/.icons/OSX/scalable/mimetypes" />
    </bean>
    <bean class="org.springframework.jmx.export.MBeanExporter"
        lazy-init="false">
        <property name="beans">
            <map>
                <entry
                    key="org.cyclopsgroup.gallerian:type=ContentListingService"
                    value-ref="gallerian.ContentListingService" />
            </map>
        </property>
    </bean>
</beans>