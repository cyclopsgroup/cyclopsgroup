<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="com.cyclopsgroup.courselist.entity">
    <class name="StudentCourse" table="c_cl_student_course">
        <meta attribute="generated-class">com.cyclopsgroup.courselist.entity.StudentCourseBase</meta>
        <id name="id" type="string" length="128" column="student_course_id">
            <generator class="uuid.hex" />
        </id>
        <property name="courseId" type="string" column="course_id" length="128" not-null="true" />
        <many-to-one name="course" class="com.cyclopsgroup.courselist.entity.Course" column="course_id" insert="false"
            update="false" />
        <property name="studentId" type="string" column="student_id" length="128" not-null="true" />
        <many-to-one name="student" class="com.cyclopsgroup.tornado.security.entity.User" column="student_id"
            insert="false" update="false" />
        <property name="score" type="float">
            <meta attribute="property-type">float</meta>
        </property>
        <property name="relationType" type="string" length="10" column="relation_type" not-null="true" />
    </class>
</hibernate-mapping>