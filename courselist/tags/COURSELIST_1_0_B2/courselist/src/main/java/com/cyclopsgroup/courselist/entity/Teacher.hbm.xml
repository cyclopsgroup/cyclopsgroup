<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="com.cyclopsgroup.courselist.entity">
    <class name="Teacher" table="c_cl_teacher">
        <meta attribute="generated-class">com.cyclopsgroup.courselist.entity.TeacherBase</meta>
        <id name="id" type="string" length="128" column="teacher_id">
            <generator class="uuid.hex" />
        </id>
        <property name="userId" column="user_id" type="string" length="128" />
        <many-to-one name="user" class="com.cyclopsgroup.tornado.security.entity.User" column="user_id" insert="false"
            update="false" />
    </class>
</hibernate-mapping>