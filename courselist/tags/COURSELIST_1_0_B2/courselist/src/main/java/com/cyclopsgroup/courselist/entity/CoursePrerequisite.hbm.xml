<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="com.cyclopsgroup.courselist.entity">
    <class name="CoursePrerequisite" table="c_cl_course_prerequisite">
        <meta attribute="generated-class">com.cyclopsgroup.courselist.entity.CoursePrerequisiteBase</meta>
        <id name="id" length="128" type="string" column="course_prerequisite_id">
            <generator class="uuid.hex" />
        </id>
        <property name="courseId" type="string">
            <column name="course_id" length="128" not-null="true" />
        </property>
        <property name="prerequisiteId" type="string">
            <column name="prerequisite_id" length="128" not-null="true" />
        </property>
    </class>
</hibernate-mapping>