<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE web-app
PUBLIC "-//Sun Microsystems, Inc.//DTD Web Application 2.2//EN"
"http://java.sun.com/j2ee/dtds/web-app_2.2.dtd">
<web-app>
	<servlet>
		<servlet-name>velocity-view</servlet-name>
		<servlet-class>
			com.cyclopsgroup.waterview.velocity.ViewServlet
		</servlet-class>
		<init-param>
			<param-name>path-mappings</param-name>
			<param-value>
				default:com/cyclopsgroup/waterview/webui
			</param-value>
		</init-param>
		<init-param>
			<param-name>layout.master</param-name>
			<param-value>default/layout/Master.vm</param-value>
		</init-param>
		<load-on-startup>1</load-on-startup>
	</servlet>
	<servlet-mapping>
		<servlet-name>velocity-view</servlet-name>
		<url-pattern>*.vm</url-pattern>
	</servlet-mapping>
</web-app>