<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping SYSTEM "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="com.cyclopsgroup.laputa.core">
    <class name="Member" table="cg_wv_member">
        <id name="id" column="member_id" type="string">
            <generator class="uuid.hex"/>
        </id>
        <property name="name" column="member_name" not-null="true" length="60"/>
        <property name="statusId" column="status_id" not-null="true" length="10"/>
        <property name="displayName" column="display_name" length="60"/>
        <!--
        <property name="email" length="120"/>
        <property name="logins" type="int"/>
        <property name="lastLogin" column="last_login" type="date"/>
        -->
        <property name="disabled" type="boolean"/>
    </class>
</hibernate-mapping>
