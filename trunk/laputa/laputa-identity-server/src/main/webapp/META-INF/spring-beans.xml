<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="
http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-2.0.xsd">
    <bean id="com.cyclopsgroup.waterview.velocity.VelocityTemplateEngine"
        class="com.cyclopsgroup.waterview.velocity.VelocityTemplateEngine"
        init-method="init">
        <constructor-arg index="0">
            <props />
        </constructor-arg>
        <constructor-arg index="1"
            ref="com.cyclopsgroup.waterview.spi.ResourceRegistry" />
    </bean>
    <bean id="com.cyclopsgroup.waterview.spi.ResourceRegistry"
        class="com.cyclopsgroup.waterview.spi.SinpleResourceRegistry">
        <constructor-arg index="0"
            value="com.cyclopsgroup.laputa.identity.webui" />
    </bean>
</beans>