<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
      "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
          "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="com.cyclopsgroup.laputa.space.pojo">
    <meta attribute="generated-class">com.cyclopsgroup.laputa.space.pojo.TestBase</meta>
    <class name="Test">
        <id name="id" type="long">
            <generator class="sequence" />
        </id>

        <property name="testName" column="test_name" type="string" />
        <property name="intValue" column="int_value" type="int" />
    </class>
</hibernate-mapping>