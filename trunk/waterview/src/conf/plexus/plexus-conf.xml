<?xml version="1.0" encoding="UTF-8"?>
<components>
    <component>
        <role>com.cyclops.waterview.Waterview</role>
        <implementation>com.cyclops.waterview.DefaultWaterview</implementation>
    </component>
    <component>
        <role>com.cyclops.waterview.RunDataFactory</role>
        <implementation>com.cyclops.waterview.rundata.RunDataFactory</implementation>
    </component>
    <component>
        <role>com.cyclops.waterview.PipelineManager</role>
        <implementation>com.cyclops.waterview.pipeline.DefaultPipelineManager</implementation>
        <configuration>
            <pipelines>
                <pipeline name="vm">
                    <valve>com.cyclops.waterview.pipeline.LinkParsingValve</valve>
                    <valve>com.cyclops.waterview.pipeline.RenderingValve</valve>
                </pipeline>
                <pipeline name="html">
                    <valve>com.cyclops.waterview.pipeline.HtmlRenderingValve</valve>
                </pipeline>
                <pipeline name="jsp">
                    <valve>ddd</valve>
                </pipeline>
                <pipeline name="png">
                    <valve>ddd</valve>
                </pipeline>
            </pipelines>
        </configuration>
    </component>
</components>
