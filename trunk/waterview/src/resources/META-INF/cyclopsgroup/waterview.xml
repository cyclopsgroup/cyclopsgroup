<?xml version="1.0" encoding="UTF-8"?>
<Waterview xmlns="com.cyclopsgroup.waterview.definition">
    <TagLibrary uri="com.cyclopsgroup.waterview.ui">
        <TagPackage className="com.cyclopsgroup.waterview.jelly.taglib.UITagPackage"/>
        <TagPackage className="com.cyclopsgroup.waterview.velocity.taglib.VelocityTagPackage"/>
    </TagLibrary>
    <Layout id="waterview.3ColumnLayout"  name="Waterview 3-column layout" description="3 columns layout provided by waterview">
        <JellyLayout script="Waterview3ColumnLayout.jelly"/>
    </Layout>
    <Layout id="waterview.DefaultLayout" name="Waterview simple layout" description="Body only">
        <JellyLayout script="WaterviewDefaultLayout.jelly"/>
    </Layout>
</Waterview>