    </organization>
    <inceptionYear>2004</inceptionYear>
    <package>com.cyclops.jmainboard</package>
    <logo>/images/jmainboard_logo.gif</logo>
    <description>JMainboard framework and some components for it</description>
    <shortDescription>A pluggable component framework</shortDescription>
    <url>http://cyclops-group.sourceforge.net/projects/jmainboard/</url>
    <siteAddress>shell.sourceforge.net</siteAddress>
    <siteDirectory>/home/groups/c/cy/cyclops-group/htdocs/projects/jmainboard/</siteDirectory>
    <repository>
        <connection>scm:cvs:pserver:anonymous@cvs.sourceforge.net:/cvsroot/cyclops-group:jmainboard</connection>
        <developerConnection>scm:cvs:ext:${maven.username}@cvs.sourceforge.net:/cvsroot/cyclops-group:jmainboard</developerConnection>
        <url>http://cvs.sourceforge.net/viewcvs.cgi/cyclops-group/jmainboard/</url>
    </repository>
    <developers>
        <developer>
            <name>Jiaqi Guo</name>
            <id>jiaqi</id>
            <email>chinajoeblack@hotmail.com</email>
            <organization/>
            <timezone>-6</timezone>
            <roles>
                <role>Java Developer</role>
            </roles>
        </developer>
    </developers>
    <dependencies>
        <dependency>
            <id>commons-beanutils</id>
            <version>1.6.1</version>
            <url>http://jakarta.apache.org/commons/beanutils</url>
        </dependency>
        <dependency>
            <id>commons-collections</id>
            <version>2.1</version>
            <url>http://jakarta.apache.org/commons/collections</url>
        </dependency>
        <dependency>
            <id>commons-digester</id>
            <version>1.5</version>
            <url>http://jakarta.apache.org/commons/digester</url>
        </dependency>
         <dependency>
            <id>commons-lang</id>
            <version>2.0</version>
            <url>http://jakarta.apache.org/commons/lang</url>
        </dependency>
        <dependency>
            <id>commons-logging</id>
            <version>1.0.3</version>
            <url>http://jakarta.apache.org/commons/logging</url>
        </dependency>
        <dependency>
            <id>junit</id>
            <version>3.8.1</version>
            <url>http://junit.org</url>
        </dependency>
        <dependency>
            <id>log4j</id>
            <version>1.2.8</version>
            <url>http://jakarta.apache.org/log4j</url>
        </dependency>
        <dependency>
            <id>velocity</id>
            <version>1.4-rc1</version>
            <url>http://jakarta.apache.org/velocity</url>
        </dependency>
        <dependency>
            <id>velocity-tools</id>
            <version>1.0</version>
            <url>http://jakarta.apache.org/velocity/tools</url>
        </dependency>
    </dependencies>
    <build>
        <nagEmailAddress>cyclops-group-develop@lists.sourceforge.net</nagEmailAddress>
        <sourceDirectory>src/java</sourceDirectory>
        <aspectSourceDirectory/>
        <unitTestSourceDirectory>src/test</unitTestSourceDirectory>
        <unitTest>
            <includes>
                <include>**/*Test.java</include>
            </includes>
        </unitTest>
        <integrationUnitTestSourceDirectory/>
    </build>
    <reports>
        <report>maven-jdepend-plugin</report>
        <report>maven-checkstyle-plugin</report>
        <report>maven-changes-plugin</report>
        <report>maven-changelog-plugin</report>
        <report>maven-file-activity-plugin</report>
        <report>maven-developer-activity-plugin</report>
        <report>maven-javadoc-plugin</report>
        <report>maven-jxr-plugin</report>
        <report>maven-junit-report-plugin</report>
        <report>maven-tasklist-plugin</report>
        <report>maven-pmd-plugin</report>
        <report>maven-simian-plugin</report>
        <report>maven-clover-plugin</report>
        <report>maven-license-plugin</report>
