<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="com.cyclopsgroup.courselist.entity">
    <class name="Course" table="c_cl_course">
        <meta attribute="generated-class">com.cyclopsgroup.courselist.entity.CourseBase</meta>
        <id name="id" type="string" length="128" column="course_id">
            <generator class="uuid.hex" />
        </id>
        <property name="prefix" type="string" length="10" not-null="true" />
        <property name="courseCode" type="string" length="10" not-null="true" column="course_code" />
        <property name="name" type="string" column="course_name" not-null="true" />
        <property name="description" type="string" length="500" />
        <property name="coRequisites" type="string" column="co_requisites" />
        <property name="isDisabled" column="is_disabled" type="boolean">
            <meta attribute="property-type">boolean</meta>
        </property>
        <property name="teacherId" column="teacher_id" type="string" length="128" />
        <many-to-one name="teacher" class="com.cyclopsgroup.tornado.security.entity.User" column="teacher_id"
            insert="false" update="false" />
        <property name="credit" type="float">
            <meta attribute="property-type">float</meta>
        </property>
        <set name="prerequisites" table="c_cl_course_prerequisite">
            <key column="course_id" />
            <many-to-many column="prerequisite_id" class="com.cyclopsgroup.courselist.entity.Course" />
        </set>
    </class>
</hibernate-mapping>