<project xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="http://maven.apache.org/xsd/maven-project-3.0.0.xsd">
    <extend>${maven.repo.local}/cyclib/poms/cyclopsgroup.pom</extend>
    <pomVersion>3</pomVersion>
    <id>clib</id>
    <name>common library</name>
    <currentVersion>SNAPSHOT</currentVersion>
    <organization>
        <name>Cyclops Group</name>
        <url>http://www.cyclopsgroup.com</url>
        <logo>/images/cyclopsgroup_logo.gif</logo>
    </organization>
    <inceptionYear>2004</inceptionYear>
    <shortDescription>Common libary is a group of basic components</shortDescription>
    <developers>
        <developer>
            <name>Jiaqi Guo</name>
            <id>jiaqi</id>
            <email>jiaqi.guo@gmail.com</email>
            <organization>Evavi</organization>
            <roles>
                <role>Developer</role>
            </roles>
            <url>http://cyclops-group.sourceforge.net</url>
            <timezone>-6</timezone>
        </developer>
    </developers>
     <build>
        <nagEmailAddress>cyclops-group-develop@lists.sourceforge.net</nagEmailAddress>
        <sourceDirectory>src/java/main</sourceDirectory>
        <unitTestSourceDirectory>src/java/test</unitTestSourceDirectory>
        <unitTest>
            <includes>
                <include>**/*Test.java</include>
            </includes>
            <resources>
                <resource>
                    <directory>${pom.build.unitTestSourceDirectory}</directory>
                    <excludes>
                        <exclude>**/*.java</exclude>
                    </excludes>
                    <filtering>false</filtering>
                </resource>
            </resources>
        </unitTest>
        <resources>
            <resource>
                <directory>${pom.build.sourceDirectory}</directory>
                <includes>
                    <include>**/*.*</include>
                </includes>
                <excludes>
                    <exclude>**/*.java</exclude>
                </excludes>
                <filtering>false</filtering>
            </resource>
            <resource>
                <directory>src/resources</directory>
                <includes>
                    <include>**/*.*</include>
                </includes>
                <filtering>false</filtering>
            </resource>
        </resources>
    </build>
</project>
