<?xml version="1.0" encoding="UTF-8"?>
<project>
    <pomVersion>3</pomVersion>
    <artifactId>cyclib</artifactId>
    <name>cyclib</name>
    <groupId>cyclib</groupId>
    <currentVersion>1.0</currentVersion>
    <inceptionYear>2004</inceptionYear>
    <logo>/images/logo.gif</logo>
    <dependencies>
        <dependency>
            <groupId>bsh</groupId>
            <artifactId>bsh</artifactId>
            <version>1.2b7</version>
            <jar>bsh-1.2b7.jar</jar>
            <type>jar</type>
        </dependency>
        <dependency>
            <groupId>commons-beanutils</groupId>
            <artifactId>commons-beanutils</artifactId>
            <version>1.6.1</version>
            <jar>commons-beanutils-1.6.1.jar</jar>
            <type>jar</type>
        </dependency>
        <dependency>
            <groupId>commons-collections</groupId>
            <artifactId>commons-collections</artifactId>
            <version>3.0</version>
            <jar>commons-collections-3.0.jar</jar>
            <type>jar</type>
        </dependency>
        <dependency>
            <groupId>commons-jelly</groupId>
            <artifactId>commons-jelly</artifactId>
            <version>20040601.171102</version>
            <jar>commons-jelly-20040601.171102.jar</jar>
            <type>jar</type>
        </dependency>
        <dependency>
            <groupId>commons-jexl</groupId>
            <artifactId>commons-jexl</artifactId>
            <version>1.0-RC1</version>
            <jar>commons-jexl-1.0-RC1.jar</jar>
            <type>jar</type>
        </dependency>
        <dependency>
            <groupId>commons-lang</groupId>
            <artifactId>commons-lang</artifactId>
            <version>2.0</version>
            <jar>commons-lang-2.0.jar</jar>
            <type>jar</type>
        </dependency>
        <dependency>
            <groupId>commons-logging</groupId>
            <artifactId>commons-logging</artifactId>
            <version>1.0.4</version>
            <jar>commons-logging-1.0.4.jar</jar>
            <type>jar</type>
        </dependency>
        <dependency>
            <groupId>dom4j</groupId>
            <artifactId>dom4j</artifactId>
            <version>1.5-beta-2</version>
            <jar>dom4j-1.5-beta-2.jar</jar>
            <type>jar</type>
        </dependency>
        <dependency>
            <groupId>junit</groupId>
            <artifactId>junit</artifactId>
            <version>3.8.1</version>
            <jar>junit-3.8.1.jar</jar>
            <type>jar</type>
        </dependency>
    </dependencies>
    <build>
        <sourceDirectory>src/java/main</sourceDirectory>
        <unitTestSourceDirectory>src/java/test</unitTestSourceDirectory>
        <unitTest>
            <includes>
                <include>**/*Test.java</include>
            </includes>
        </unitTest>
    </build>
</project>