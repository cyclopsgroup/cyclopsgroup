    <build>
        <sourceDirectory>src/java</sourceDirectory>
        <aspectSourceDirectory/>
        <unitTestSourceDirectory>src/test</unitTestSourceDirectory>
        <unitTest>
            <includes>
                <include>**/*Test.java</include>
            </includes>
        </unitTest>
        <integrationUnitTestSourceDirectory>src/rttest</integrationUnitTestSourceDirectory>
        <integrationUnitTest>
            <includes>
                <include>**/*Test.java</include>
            </includes>
        </integrationUnitTest>
        <resources>
            <resource>
                <directory>src/webapp/WEB-INF</directory>
                <includes>
                    <include>conf/**/*</include>
                </includes>
            </resource>
            <resource>
                <directory>src/template/velocity</directory>
                <includes>
                    <include>**/*.vm</include>
                </includes>
            </resource>
        </resources>
        <jars/>
    </build>
