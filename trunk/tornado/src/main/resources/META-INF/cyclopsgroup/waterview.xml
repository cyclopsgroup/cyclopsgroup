<?xml version="1.0" encoding="UTF-8"?>
<Waterview xmlns="http://waterview.cyclopsgroup.com/definition">
    <ModulePackage alias="tornado" name="com.cyclopsgroup.tornado.ui" />
    <TagLibrary uri="http://tornado.cyclopsgroup.com/tool">
        <TagPackage className="com.cyclopsgroup.tornado.hibernate.taglib.HibernateToolTagPackage" />
    </TagLibrary>
    <TagLibrary uri="http://tornado.cyclopsgroup.com/ui">
        <TagPackage className="com.cyclopsgroup.tornado.hibernate.taglib.HibernateUITagPackage" />
    </TagLibrary>
    <StyleSheet name="tornado.stylesheet.verdure" path="/tornado/verdure-style.css"
        description="Tornado verdure stylesheet" />
    <StyleSheet name="tornado.stylesheet.google" path="/tornado/google-style.css"
        description="Tornado google stylesheet" />
    <StyleSheet name="tornado.stylesheet.leaf" path="/tornado/leaf-style.css" description="Tornado leaf stylesheet" />
    <Theme name="tornado.theme.default" iconset="waterview.icons.default" stylesheet="waterview.stylesheet.default"
        description="Tornado default theme">
        <Layout name="layout.default">
            <JellyLayout script="/tornado/DefaultLayout.jelly" />
        </Layout>
    </Theme>
</Waterview>