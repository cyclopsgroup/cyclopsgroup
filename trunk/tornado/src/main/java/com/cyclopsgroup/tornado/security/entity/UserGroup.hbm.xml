<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="com.cyclopsgroup.tornado.security.entity">
    <class name="UserGroup" table="c_t_user_group">
        <meta attribute="generated-class">com.cyclopsgroup.tornado.security.entity.UserGroupBase</meta>
        <id name="id" length="128" type="string" column="user_group_id">
            <generator class="uuid.hex" />
        </id>
        <property name="userId" type="string" column="user_id" not-null="true" length="128" />
        <property name="groupId" type="string" column="group_id" not-null="true" length="128" />
    </class>
</hibernate-mapping>