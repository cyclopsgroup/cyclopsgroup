<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="com.cyclopsgroup.tornado.security.entity">
    <class name="RolePermission" table="c_t_role_permission">
        <meta attribute="generated-class">com.cyclopsgroup.tornado.security.entity.RolePermissionBase</meta>
        <id name="id" length="128" type="string" column="role_permission_id">
            <generator class="uuid.hex" />
        </id>
        <property name="roleId" type="string">
            <column name="role_id" length="128" not-null="true" />
        </property>
        <property name="permissionType" type="string" column="permission_Type" not-null="true" />
        <property name="permission" type="string" not-null="true" />
    </class>
</hibernate-mapping>