<?xml version="1.0" encoding="ISO-8859-1"?>
<project xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:noNamespaceSchemaLocation="http://maven.apache.org/xsd/maven-project-3.0.0.xsd">
    <extend>${maven.repo.local}/com.cyclopsgroup/poms/cyclopsgroup-1.0.pom</extend>
    <pomVersion>3</pomVersion>
    <id>maven-${maven.amulets.subproject}-plugin</id>
    <name>Maven ${maven.amulets.subproject} plugin</name>
    <currentVersion>SNAPSHOT</currentVersion>
    <organization>
        <name>Cyclops Group</name>
        <url>http://www.cyclopsgroup.com</url>
        <logo>/images/cyclopsgroup_logo.png</logo>
    </organization>
    <inceptionYear>2003</inceptionYear>
    <logo>/images/amulets_logo.png</logo>
    <shortDescription>Java Project Management Tools</shortDescription>
    <siteDirectory>${maven.cyclopsgroup.site.directory}/projects/amulets/multiproject/${pom.artifactId}</siteDirectory>
    <repository>
        <connection>
            scm:cvs:pserver:anonymous@cvs.sourceforge.net:/cvsroot/cyclops-group:amulets/${maven.amulets.subproject}
        </connection>
        <developerConnection>
            scm:cvs:ext:your-name@cvs.sourceforge.net:/cvsroot/cyclops-group:amulets/${maven.amulets.subproject}
        </developerConnection>
        <url>http://fisheye.cenqua.com/viewrep/cyclops-group/amulets/${maven.amulets.subproject}</url>
    </repository>
    <developers />
    <dependencies />
    <build />
</project>
