    </versions>
    <developers>
        <developer>
            <name>Guo, Jiaqi</name>
            <id>jiaqi</id>
            <email>jiaqi.guo@gmail.com</email>
            <organization>Evavi</organization>
            <roles>
                <role>Developer</role>
            </roles>
            <timezone>-6</timezone>
        </developer>
    </developers>
    <dependencies>
        <dependency>
            <id>commons-io</id>
            <version>1.0</version>
        </dependency>
        <dependency>
