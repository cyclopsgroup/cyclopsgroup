<?xml version="1.0" encoding="ISO-8859-1"?>
<project xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="http://maven.apache.org/xsd/maven-project-3.0.0.xsd">
	<extend>${maven.repo.local}/clib/poms/cyclopsgroup.pom</extend>
	<pomVersion>3</pomVersion>
	<id>amulets</id>
	<name>amulets</name>
	<groupId>amulets</groupId>
	<currentVersion>SNAPSHOT</currentVersion>
	<organization>
		<name>Cyclops Group</name>
	</organization>
	<inceptionYear>2003</inceptionYear>
	<logo>/images/amulets_logo.gif</logo>
	<shortDescription>Java Project Management Tools</shortDescription>
	<url>http://cyclops-group.sourceforge.net/projects/amulets/multiproject/${pom.artifactId}</url>
	<siteDirectory>/home/groups/c/cy/cyclops-group/htdocs/projects/amulets/multiproject/${pom.artifactId}/</siteDirectory>
	<developers/>
	<dependencies/>
	<build>
		<sourceDirectory>src/java/main</sourceDirectory>
		<unitTestSourceDirectory>src/java/test</unitTestSourceDirectory>
		<unitTest>
			<includes>
				<include>**/*Test.java</include>
			</includes>
		</unitTest>
		<resources>
			<resource>
				<directory>src/plugin-resources</directory>
				<targetPath>plugin-resources</targetPath>
			</resource>
			<resource>
				<directory>.</directory>
				<includes>
					<include>plugin.jelly</include>
					<include>plugin.properties</include>
					<include>project.properties</include>
					<include>project.xml</include>
				</includes>
			</resource>
		</resources>
	</build>
	<reports>
		<report>maven-changes-plugin</report>
		<report>maven-changelog-plugin</report>
		<report>maven-file-activity-plugin</report>
		<report>maven-developer-activity-plugin</report>
		<report>maven-license-plugin</report>
	</reports>
</project>
