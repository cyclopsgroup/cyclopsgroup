<?xml version="1.0" encoding="UTF-8"?>
<span xmlns:w="http://waterview.cyclopsgroup.com/ui" xmlns:j="jelly:core">
    <j:set var="fieldType" value="text" />
    <j:if test="${fieldTag.password}">
        <j:set var="fieldType" value="password" />
    </j:if>
    <input type="${fieldType}" name="${field.name}" value="${field.value}" size="${fieldTag.getSize()}"
        class="waterviewFormInput" />
    <w:JellyScript path="/waterview/FormInputFieldSuffix.jelly" />
</span>