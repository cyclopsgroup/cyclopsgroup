<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="com.cyclopsgroup.tornado.security.entity">
    <class name="UserRole" table="c_t_user_role">
        <meta attribute="generated-class">com.cyclopsgroup.tornado.security.entity.UserRoleBase</meta>
        <id name="id" length="128" type="string" column="user_role_id">
            <generator class="uuid.hex" />
        </id>
        <property name="userId" type="string" column="user_id" not-null="true" length="128" />
        <property name="roleId" type="string" column="role_id" not-null="true" length="128" />
        <many-to-one name="role" class="com.cyclopsgroup.tornado.security.entity.Role" column="role_id" insert="false"
            update="false" />
    </class>
</hibernate-mapping>